    "n=int(input())\n",
    "arr=list(map(int,raw_input().split()))\n",
    "swaps=0\n",
    "for i in range(0,len(arr)-1):\n",
    "    while arr[i]!=i+1:\n",
    "        t=arr[arr[i]-1]\n",
    "        arr[arr[i]-1]=arr[i]\n",
    "        arr[i]=t\n",
    "        swaps+=1\n",
    "print(arr)\n",
    "print(swaps)\n"
   ]
